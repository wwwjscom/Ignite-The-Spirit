<h1>New bio</h1>

<% form_for(@bio) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :thumb_image_url %><br />
    <%= f.text_field :thumb_image_url %>
  </p>
  <p>
    <%= f.label :full_image_url %><br />
    <%= f.text_field :full_image_url %>
  </p>
  <p>
    <%= f.label :bio %><br />
    <%= f.text_area :bio %>
  </p>
  <p>
    <%= f.label :cal_year_id %><br />
    <%= f.collection_select :cal_year_id, CalYear.all, :id, :year %>
  </p>
  <p>
    <%= f.label :month_id %><br />
    <%= f.collection_select :month_id, Month.all, :id, :month %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>
