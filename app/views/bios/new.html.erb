<h1>New bio</h1>

<% form_for(@bio) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :thumb_image_url %><br />
    <%= f.text_field :thumb_image_url %>
  </p>
  <p>
    <%= f.label :full_image_url %><br />
    <%= f.text_field :full_image_url %>
  </p>
  <p>
    <%= f.label :bio %><br />
    <%= f.text_area :bio %>
  </p>
  <p>
    <%= f.label :cal_year_id %><br />
    <%= f.text_field :cal_year_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', bios_path %>